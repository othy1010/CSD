<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Decision.
    -->
    <changeSet id="20230614120256-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="proposal_id"
                                 baseTableName="decision"
                                 constraintName="fk_decision__proposal_id"
                                 referencedColumnNames="id"
                                 referencedTableName="proposal"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="decision"
                                 constraintName="fk_decision__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="involved_user"/>
    </changeSet>
</databaseChangeLog>
